{
  "type": "kafka",
  "spec": {
    "ioConfig": {
      "type": "kafka",
      "consumerProperties": {
        "bootstrap.servers": "kafka:9092"
      },
      "topic": "stock_prices",
      "inputFormat": {
        "type": "json"
      },
      "useEarliestOffset": true
    },
    "tuningConfig": {
      "type": "kafka",
      "maxRowsPerSegment": 5000000
    },
    "dataSchema": {
      "dataSource": "stocks_datasource",
      "timestampSpec": {
        "column": "timestamp",
        "format": "posix"
      },
      "dimensionsSpec": {
        "dimensions": [
          {
            "type": "string",
            "name": "symbol"
          },
          {
            "type": "string",
            "name": "source"
          },
          {
            "type": "double",
            "name": "price"
          },
          {
            "type": "double",
            "name": "change"
          },
          {
            "type": "string",
            "name": "change_percent"
          },
          {
            "type": "long",
            "name": "volume"
          }
        ]
      },
      "granularitySpec": {
        "type": "uniform",
        "segmentGranularity": "HOUR",
        "queryGranularity": "MINUTE",
        "rollup": false
      }
    }
  }
}
