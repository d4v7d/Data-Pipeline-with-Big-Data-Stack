{
  "source": {
    "type": "airflow",
    "serviceName": "airflow_source",
    "serviceConnection": {
      "config": {
        "type": "Airflow",
        "hostPort": "http://airflow:8080",
        "username": "admin",
        "password": "admin"
      }
    },
    "sourceConfig": {
      "config": {
        "type": "PipelineMetadata",
        "includeTags": true,
        "includeLineage": true,
        "pipelineFilterPattern": {
          "includes": [".*"]
        }
      }
    }
  },
  "sink": {
    "type": "metadata-rest",
    "config": {
      "protocol": "http",
      "host": "openmetadata",
      "port": 8585
    }
  },
  "workflowConfig": {
    "openMetadataServerConfig": {
      "hostPort": "http://openmetadata:8585/api",
      "authProvider": "openmetadata"
    }
  }
}
